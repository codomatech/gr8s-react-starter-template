    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "isbot": "^5.1.23",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.5.2",
    "typescript-eslint": "^8.26.1"
  },
  "devDependencies": {
    "@codomatech/gr8s-tools": "^0.0.21",
    "@react-router/dev": "^7.3.0",
    "@types/node": "^20.17.24",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
